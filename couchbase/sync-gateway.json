 {
  "log": ["*"],
 "adminInterface":":4985",
 "MaxFileDescriptors":100000,
 "logFilePath":"sync_log.text",
 "maxCouchbaseConnections":100,
  "databases": {
    "default": {
      "server": "http://etools-staging-couchbase.westeurope.cloudapp.azure.com:8091",
      "bucket": "default",
       "sync": `function(doc){
          if(doc.type == "user") {

            requireUser(doc.type);
            access(doc.type, "users");
            channel("users");
          }
          else if(doc.partner_name && doc.country){
            channel(doc.partner_name+"-"+doc.country);
          }
        }`
 ,
      "users": {        
                 "GUEST": {
                    "disabled": true 
                }
            }
    }
  }
}